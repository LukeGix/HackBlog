<!DOCTYPE html>
<html>
<head>
	<title>Github Writeups</title>
	<meta charset="utf-8">
	<link rel="stylesheet" type="text/css" href="/git.css">
	<%- include('head.ejs')%>
	<%if(content !== undefined){%>
		<link src="https://cdn.rawgit.com/showdownjs/showdown/1.9.0/dist/showdown.min.js
" type="text/javascript" />
	<%}%>
</head>
<body>
	<div class="mask">
			<div class="container-fluid text-white">
				<%- include('logo.ejs')%>
				<%- include('header.ejs')%>
				<div class="container-fluid">
					<%var a = contents; %>
					<%if(a !== undefined){%>
					<div class="container-fluid text-center">
						<% for(var i=0; i < a.length; i++){%>
							<%if(prefix !== undefined){%>
								<a href = "<%= '/MyBlog/git/'+prefix+'/'+a[i]%>" ><%- a[i]%></a><br>
							<%}else{%>
								<a href = "<%= '/MyBlog/git/'+a[i]%>" ><%- a[i]%></a><br>
							<%}%>
						<%}%>
					</div>
					<%}%>
					
					<%if(content !== undefined){%>
							<div class="container-fluid text-white">
								<div class="container-fluid ctfcontent">
									<%- content%>
								</div>
							</div>
						<%}%>	
					
					<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
				</div>	
			</div>
		<%- include('footer.ejs')%>
	</div>
</body>
</html>
