<!DOCTYPE html>
<html>
<head>
	<title>Github Writeups</title>
	<%- include('head.ejs')%>
	<%if(content !== undefined){%>
		<link src="https://cdn.rawgit.com/showdownjs/showdown/1.9.0/dist/showdown.min.js
" type="text/javascript" />
	<%}%>
</head>
<body>
	<div class="mask">
			<div class="container-fluid text-white">
				<h1 class="text-white text-center">Logo</h1>
				<%- include('header.ejs')%>
				<div class="container-fluid text-center">
					<p>Writeup contents soon!</p>
					<%var a = contents; %>
					<%if(a !== undefined){%>
						<% for(var i=0; i < a.length; i++){%>
							<%if(prefix !== undefined){%>
								<a href = "<%= 'https://localhost:1337/MyBlog/git/'+prefix+'/'+a[i]%>" ><%- a[i]%></a><br>
							<%}else{%>
								<a href = "<%= 'https://localhost:1337/MyBlog/git/'+a[i]%>" ><%- a[i]%></a><br>
							<%}%>
						<%}%>
					<%}%>
					
					<%if(content !== undefined){%>
							<div class="container-fluid text-white text-center">
								<h1>Content of the file</h1>
								<div class="container-fluid">
									<%- content%>
								</div>
							</div>
						<%}%>	
					
					<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
				</div>	
			</div>
		<%- include('footer.ejs')%>
	</div>
</body>
</html>
